	 margin:0 auto;
}

#menu-main-nav {
	float:right;
	width:535px;
	height:30px;
	margin:8px 0 0 0;
	list-style:none outside none;
}

#menu-main-nav li {
	float: left;
	height:30px;
	margin:0;
	position:relative;
}

#menu-main-nav img {
	cursor:pointer;
	display:block;
	text-align:center;
	height:18px

}


#menu-main-nav li:hover {
	background:url(../images/common/main-nav-selected.gif) no-repeat scroll center bottom transparent;
}

#footerNavigation {
