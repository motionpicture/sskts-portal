body{
	margin:0;
	padding:0;
	font-size:12px;
	letter-spacing:0.15em;
	line-height:150%;
	color:#333333;
	font-family:"Arial","M+1cregular","メイリオ","ヒラギノ角ゴ Pro W3","Verdada",Osaka,sans-serif;
	/*background-color:#D9E6E8;*/
	background-color:ffffff;
}

a{
	color:#909EA0;
	text-decoration:underline;
}

a:hover{
	text-decoration:none;
}


img{
	border:none;
}

h3{
	padding:0;
	font-size:30px;
	font-weight:normal;
	clear:both;
	color:#333333;
	padding:60px 0 0 0;
	margin:0;
	font-family:"Courier New","M+1cregular","メイリオ","ヒラギノ角ゴ Pro W3","Verdada",Osaka,sans-serif;

}

#wrap{
	z-index:1;
}

ul{
	padding:0;
}

li{	
	list-style-type:none;
}

#back_to_top{
	float:right;
	margin:40px 0 0 0;
	
}

.dot_line{
	margin:30px 0 0 0;
}

.dot_line2{
	padding:40px 0 0 0;
}

.clear{
	clear:both;
	margin:0;
	padding:0;
}

/*ナビゲーションホバー*/

a:hover img {
	opacity:0.30;
	-moz-opacity: 0.30; /* Firefox */
	filter: alpha(opacity=30); /* IE6/7 */
	-ms-filter: "alpha(opacity=30)"; /* IE8 */	
	}	

.active {
	opacity: 0.30;
	-moz-opacity: 0.30; /* Firefox */
	filter: alpha(opacity=30); /* IE6/7 */
	-ms-filter: "alpha(opacity=30)"; /* IE8 */
	}
	
.active a:hover img {
	opacity:1;
	-moz-opacity: 1; /* Firefox */
	filter: alpha(opacity=1); /* IE6/7 */
	-ms-filter: "alpha(opacity=1)"; /* IE8 */	
	}	
	
.active2 {
	opacity: 1;
	-moz-opacity: 1; /* Firefox */
	filter: alpha(opacity=1); /* IE6/7 */
	-ms-filter: "alpha(opacity=1)"; /* IE8 */
	}
	
.active2 a:hover img {
	opacity:1;
	-moz-opacity: 1; /* Firefox */
	filter: alpha(opacity=1); /* IE6/7 */
	-ms-filter: "alpha(opacity=1)"; /* IE8 */	
	}		
	
/*ナビゲーションホバーここまで*/


/*反転時の文字色・背景色 */
::selection {
background: #D1E6EA;　　/* 背景色*/
color:#666666;　　/* 文字色*/
}
/* Firefox対応*/
::-moz-selection {
background: #D1E6EA;　　/* 背景色*/
color:#666666; 　　/* 文字色*/
}

 
