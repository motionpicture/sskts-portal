#AuthType Basic
#AuthName "Input your ID and Password."
#AuthUserFile /var/www/vhosts/cinemasunshine.co.jp/httpdocs/cine_cms/.htpasswd
#require valid-user
<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /cine_cms/app/webroot
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]
</IfModule>